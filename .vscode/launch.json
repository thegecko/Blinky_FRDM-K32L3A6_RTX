{
    "configurations": [
        {
            "name": "Embedded Debug",
            "type": "embedded-debug",
            "request": "launch",
            "serialNumber": "${command:device-manager.getSerialNumber}",
            "program": "${command:embedded-debug.getBinaryFile}",
            "cmsisPack": "${workspaceFolder}/NXP.K32L3A60_DFP.15.0.0.pack",
            "debugFrom": "main",
            "pathSubstitutions": [{
                "find": "/workspace/Blinky_FRDM-K32L3A6_RTX",
                "replace": "${workspaceFolder}"
            }]
        }
    ]
}